print("JEFFRIN PONSHIYA")
def print_e_pattern():
  rows = 7
  cols = 5
  for i in range (rows):
      for j in range(cols):
        if i == 0 or i == rows - 1 or i == rows // 2:
          print('*', end='')
        elif j == 0:
          print('*', end='')
        else:
          print(' ', end='')
      print()
print_e_pattern() 
